<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chatroom.testchatjfx.ServerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane fx:id="Anchor" prefHeight="200.0" prefWidth="200.0" />

    <Label fx:id="welcomeText" />
   <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
